{
  "id": "dimensional-weather",
  "title": "Dimensional Weather",
  "description": "A dynamic weather system for Foundry VTT, providing a modular system for developing your own climes, terrains, and atmospheric phenomenon.",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": "13",
    "verified": "13",
    "maximum": "13"
  },
  "authors": [
    {
      "name": "Chris Allbritton",
      "email": "callbritton@mac.com",
      "url": "https://github.com/ctbritt"
    }
  ],
  "esmodules": [
    "scripts/services/weather-description.js",
    "scripts/api.js",
    "scripts/command-system.js",
    "scripts/main.js",
    "scripts/scene-manager.js",
    "scripts/settings.js",
    "scripts/state-manager.js",
    "scripts/time-utils.js",
    "scripts/ui-controller.js",
    "scripts/utils.js",
    "scripts/weather-calculator.js",
    "scripts/weather-engine.js"
  ],
  "styles": [
    "styles/dimensional-weather.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    },
    {
      "lang": "es",
      "name": "Español",
      "path": "lang/es.json",
      "flags": {}
    },
    {
      "lang": "fr",
      "name": "Français",
      "path": "lang/fr.json",
      "flags": {}
    },
    {
      "lang": "uk",
      "name": "Українська",
      "path": "lang/uk.json",
      "flags": {}
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "bugs": "https://github.com/ctbritt/dimensional-weather/issues",
  "media": [
    {
      "type": "icon",
      "url": "https://avatars2.githubusercontent.com/u/71292812?s=400&u=ccdb4eeb7abf551ca8f314e5a9bfd0479a4d3d41&v=4"
    }
  ],
  "changelog": "CHANGELOG.md",
  "socket": false,
  "relationships": {
    "requires": [
      {
        "id": "_chatcommands",
        "type": "module",
        "manifest": "https://gitlab.com/woodentavern/foundryvtt-chat-command-lib/-/raw/master/src/module.json",
        "compatibility": {
          "verified": "2.0.1"
        }
      },
      {
        "id": "lib-wrapper",
        "type": "module",
        "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/download/v1.12.11.0/module.json",
        "compatibility": {
          "verified": "1.12.11.0"
        }
      }
    ]
  }
}